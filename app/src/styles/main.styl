.container 
  max-width 100%


#map-canvas 
  height 500px
  width 100%

#chart
  display block
  &:hover
    cursor move
  &:active
    cursor move


#chart svg
  height 300px

  .nv-bar:nth-child(13)
    fill red !important
    stroke red !important


.selected
  stroke red !important
  fill red !important

